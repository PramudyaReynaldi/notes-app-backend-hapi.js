{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "import/no-extraneous-dependencies": ["error", {
            "devDependencies": false,
            "optionalDependencies": false,
            "peerDependencies": false,
            "bundledDependencies": false
        }],
        "no-console": "off",
        "no-unused-vars": "off",
        "no-undef": "off"

    }
}
